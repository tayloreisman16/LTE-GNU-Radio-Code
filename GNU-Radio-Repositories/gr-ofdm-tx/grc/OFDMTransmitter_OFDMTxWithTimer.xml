<?xml version="1.0"?>
<block>
  <name>OFDM Tx With Timer</name>
  <key>OFDMTransmitter_OFDMTxWithTimer</key>
  <category>[OFDMTransmitter]</category>
  <import>import OFDMTransmitter</import>
  <make>OFDMTransmitter.OFDMTxWithTimer($case, $pickle_directory)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Case</name>
    <key>case</key>
    <type>int</type>
  </param>

  <param>
    <name>Pickle Directory</name>
    <key>pickle_directory</key>
    <type>string</type>
  </param>
  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->


  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
