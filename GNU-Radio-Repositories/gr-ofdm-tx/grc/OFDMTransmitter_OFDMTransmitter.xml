<?xml version="1.0"?>
<block>
  <name>OFDMTransmitter</name>
  <key>OFDMTransmitter_OFDMTransmitter</key>
  <category>[OFDMTransmitter]</category>
  <import>import OFDMTransmitter</import>
  <make>OFDMTransmitter.OFDMTransmitter($case, $num_ofdm_symb, $fft_size, $num_data_bins)</make>

  <param>
    <name>Case</name>
    <key>case</key>
    <type>int</type>
  </param>

  <param>
    <name>Number of OFDM Symbols</name>
    <key>num_ofdm_symb</key>
    <type>int</type>
  </param>

  <param>
    <name>FFT Size</name>
    <key>fft_size</key>
    <type>int</type>
  </param>

  <param>
    <name>Number of Data Bins</name>
    <key>num_data_bins</key>
    <type>int</type>
  </param>

  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
