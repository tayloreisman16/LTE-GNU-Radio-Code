<?xml version="1.0"?>
<block>
  <name>Synchronize and Channel Estimate</name>
  <key>OFDMReceiver_SynchronizeAndEstimate</key>
  <category>[OFDMReceiver]</category>
  <import>import OFDMReceiver</import>
  <make>OFDMReceiver.SynchronizeAndEstimate($case)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->

  <param>
    <name>Case</name>
    <key>case</key>
    <type>int</type>
  </param>
  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
